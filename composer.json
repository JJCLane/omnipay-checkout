{
    "name": "jjclane/omnipay-checkout",
    "description": "Checkout driver for the Omnipay PHP payment processing library",
    "keywords": [
        "jjclane",
        "omnipay-checkout"
    ],
    "homepage": "https://github.com/JJCLane/omnipay-checkout",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Benjamin BeganoviÄ‡",
            "email": "ben@invoiceninja.com",
            "role": "Developer"
        },
        {
            "name": "Jordan Lane",
            "email": "contact@jordanlane.me",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1|^8.1",
        "omnipay/common": "^3",
        "symfony/http-client": "^7.1"
    },
    "require-dev": {
        "http-interop/http-factory-guzzle": "^1.2",
        "omnipay/tests": "^4"
    },
    "autoload": {
        "psr-4": {
            "Omnipay\\Checkout\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Omnipay\\Checkout\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "php-http/discovery": true
        }
    }
}
